{
  "version": 2,
  "name": "focus",
  "builds": [
      {
          "use": "@now/static-build",
          "src": "package.json"
      }
  ],
  "routes": [
      {
          "src": "(.*).js",
          "dest": "/$1.js"
      },
      {
          "src": "(.*).scss",
          "dest": "/$1.scss"
      },
      {
          "src": "(.*).css",
          "dest": "/$1.css"
      },
      {
          "src": "(.*).json",
          "dest": "/$1.json"
      },
      {
          "src": "/.*",
          "dest": "index.html"
      }
  ]
}
